{
  "name": "react-ts-webpack",
  "version": "1.0.0",
  "description": "based on react + ts + webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue --ignore-path .eslintignore .",
    "lint-f": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue --ignore-path .eslintignore .",
    "test": "jest",
    "test-c": "jest --coverage",
    "pretty": "prettier --write ."
  },
  "keywords": [
    "react.js",
    "typescript",
    "webpack"
  ],
  "author": "cuiko",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,vue}": [
      "npm run pretty",
      "npm run lint-f",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@cuiko/eslint-config": "1.0.9",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^5.2.0",
    "eslint": "^7.23.0",
    "eslint-plugin-react": "7.21.4",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.4.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^11.0.1",
    "source-map-loader": "^2.0.1",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "typescript": "3.5.3",
    "webpack": "^5.28.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "classnames": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  }
}
